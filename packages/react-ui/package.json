        "test": "umi-test",
        "test:watch": "umi-test --watch --coverage",
        "prepublishOnly": "yarn build",
        "test:coverage": "umi-test --coverage"
    },
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "typings": "dist/index.d.ts",
    "files": [
